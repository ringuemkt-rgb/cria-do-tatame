      "src/core/CombatEngine.js": "export class CombatEngine {\n    static resolveMove(attacker, defender, move) {\n        const successChance = (attacker.technique * move.mod) / (defender.technique * 0.5);\n        const roll = Math.random() * 100;\n        if (attacker.stamina < move.cost) return { success: false, msg: 'Sem g√°s!' };\n        attacker.stamina -= move.cost;\n        if (roll <= successChance) {\n            defender.takeDamage(move.damage);\n            return { success: true, msg: `${attacker.name} encaixou um ${move.name}!` };\n        }\n        return { success: false, msg: `${defender.name} defendeu!` };\n    }\n}",
